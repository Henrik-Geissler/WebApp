{
  "name": "webapp",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "Henrik Geissler"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Henrik-Geissler/WebApp.git"
  },
  "homepage": "https://henrik-geissler.github.io/WebApp",
  "dependencies": {
    "@capacitor/core": "1.5.2",
    "@ionic/react": "^5.0.7",
    "@ionic/react-router": "^5.0.7",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.3",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.24",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint-plugin-react": "^7.19.0",
    "ionicons": "^5.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "typescript": "3.7.4"
  },
  "scripts": {
    "start": "PORT=3000 react-scripts start",
    "build": "react-scripts build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "test": "yarn test-async:all",
    "test:e2e": "PORT=3000 NODE_ENV=debug cucumber-js features",
    "test:unit": "react-scripts test --watchAll --coverage",
    "test-async:all": "yarn test-async:unit && yarn test-async:e2e",
    "test-async:unit": "echo \"cd \"`pwd`\";yarn test:unit;exit;\" > \"`pwd`\"/test/UNIT_Test.command; chmod +x \"`pwd`\"/test/UNIT_Test.command; open \"`pwd`\"/test/UNIT_Test.command",
    "test-async:e2e": "echo \"cd \"`pwd`\";yarn test:e2e;exit;\" > \"`pwd`\"/test/E2E_Test.command; chmod +x \"`pwd`\"/test/E2E_Test.command; open \"`pwd`\"/test/E2E_Test.command",
    "test-ci:unit": "react-scripts test --watchAll=false --forceExit --detectOpenHandles --ci",
    "test-ci:e2e": "PORT=3000 cucumber-js features --exit",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "1.5.2",
    "axe-puppeteer": "^1.0.0",
    "axe-reports": "^1.1.11",
    "cucumber": "5.0.2",
    "expect": "24.5.0",
    "gh-pages": "^2.2.0",
    "pixelmatch": "^5.0.0",
    "prettier": "^1.19.1",
    "puppeteer": "1.9.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/{!(index),}.tsx"
    ]
  },
  "description": "An Ionic project"
}
